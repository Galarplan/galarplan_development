<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_report_state_saving" model="ir.actions.report">
        <field name="name">Estado de Cuenta</field>
        <field name="model">account.saving</field> <!-- Modelo corregido -->
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">planes_ahorro.report_saving_state</field>
        <field name="print_report_name">'Estado de cuenta - %s' % (object.name)</field>
        <field name="binding_model_id" ref="model_account_saving" /> <!-- ID corregido -->
        <field name="binding_type">report</field>
    </record>

    <record id="action_report_state_saving_xlsx" model="ir.actions.report">
        <field name="name">Estado de Cuenta (Excel)</field>
        <field name="model">account.saving</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">planes_ahorro.report_saving_state_xlsx</field>
        <field name="print_report_name">'Estado de cuenta - %s.xlsx' % (object.name)</field>
    </record>

</odoo>