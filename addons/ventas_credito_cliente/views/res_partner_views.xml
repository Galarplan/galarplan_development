<odoo>
    <record id="view_partner_form_credit" model="ir.ui.view">
        <field name="name">res.partner.form.credit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_account_receivable_id']" position="after">
                <field name="property_account_receivable_credit_id"/>
                <field name="property_account_financial_bank_id"/>
                <field name="property_account_direct_funds_id"/>
                <field name="propoerty_account_adjudicated_id"/>
            </xpath>
        </field>
    </record>
</odoo>